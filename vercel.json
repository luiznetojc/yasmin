{
	"version": 2,
	"builds": [
	  {
		"src": "package.json",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "dist/yasmin/browser"
		}
	  }
	],
	"rewrites": [
	  {
		"source": "/(.*)",
		"destination": "/index.html"
	  }
	],
	"headers": [
	  {
		"source": "/(.*)\\.js",
		"headers": [
		  { "key": "Content-Type", "value": "application/javascript" }
		]
	  }
	]
  }
  